aws s3 cp s3://oms-install/weblogic/auto-restart/jms/weblogic-nodemgr.service /etc/systemd/system/weblogic-nodemgr.service
chmod 644 /etc/systemd/system/weblogic-nodemgr.service

aws s3 cp s3://oms-install/userdata/start_jms_nodemgr.sh /u01/app/oracle/config/script/start_jms_nodemgr.sh
chown weblogic: /u01/app/oracle/config/script/start_jms_nodemgr.sh
chmod 755 /u01/app/oracle/config/script/start_jms_nodemgr.sh
sudo systemctl enable weblogic-nodemgr
